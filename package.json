{
    "name"        : "{{name}}",
    "description" : "{{description}}",
    "license"     : "Apache-2.0",
    "homepage"    : "https://github.com/{{user}}/{{name}}",
    "main"        : "{{name}}.min.js",
    "version"     : "0.0.1",
    "engines": {
        "node" : ">=4.2.4"
    },
    "maintainers": [{
        "name"  : "David Volm",
        "email" : "david@volminator.com",
        "web"   : "http://daxxog.com/"
    }],
    "repository": {
        "type" : "git",
        "url"  : "https://github.com/{{user}}/{{name}}.git"
    },
    "devDependencies": {
        "bitfactory": "*",
        "stoptime": "*",
        "uglify-js": "2.4.24",
        "vows": "*"
    },
    "scripts": {
        "build": "node make.js; cat .gitignore | grep -v {{name}}.min.js > .npmignore",
        "test": "vows test/*",
        "start": "node cli.js",
        "prepublish": "npm run build"
    },
    "files": [
        "cli.min.js",
        "{{name}}.min.js",
        "bin/{{name}}"
    ],
    "bin": {
        "{{name}}" : "bin/{{name}}"
    }
}
